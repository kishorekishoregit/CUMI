                </table>
            </div>
            <style>
                #dynamic-tableUpload td:empty {
                    display: none;
                }
            </style>
        </div>
    </div>


    <div class="modal" id="modalview">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="popup_header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h3 class="header smaller lighter blue">PRN Details</h3>

                </div>
                <div class="modal-body">
                    <div class="tabbable">


                        <div class="tab-content no-border padding-24">

                            <div id="upload" class="tab-pane active">
                                <div id="fuelux-wizard-container" class="no-steps-container">
                                    <div class="col-xs-12 clearfix">
                                        <div class="table-responsive">
                                            <table id="dynamic-tableDetails" class="table table-striped table-bordered table-hover">
                                                <thead>
                                                    <tr></tr>
                                                </thead>
                                                <tbody id="tbody"></tbody>

                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>



                        </div>
                    </div>
                </div>
                <div class="modal-footer">

                </div>
            </div>
        </div>
    </div>

</div><!-- /.row -->
@*POPUP*@

<div class="modal" id="modalview1">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="popup_header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h3 class="header smaller lighter blue">RM Label Print</h3>
            </div>
            <div class="modal-body">
                <div class="tabbable">


                    <div class="tab-content no-border padding-24">

                        <div id="upload" class="tab-pane active">
                            <div id="fuelux-wizard-container" class="no-steps-container">
                                <form class="form-horizontal clearfix" id="" name="myform" action="@Url.Content("~/Transaction/ViewProductionPlan")" method="post">

                                    <div class="col-xs-12 clearfix">
                                        <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                            <label class="control-label col-lg-4 no-padding-right" for="txtlotno">Lotno<span class="text-danger"></span>:</label>

                                            <div class="col-lg-8">
                                                <div class="clearfix">
                                                    <input style="text-transform:uppercase" type="text" id="txtslotno" name="txtslotno" class="form-control" maxlength="100" placeholder="" readonly>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                            <label class="control-label col-lg-4 no-padding-right" for="txtnetweight">NetWeight<span class="text-danger"></span>:</label>

                                            <div class="col-lg-8">
                                                <div class="clearfix">
                                                    <input style="text-transform:uppercase" type="text" id="txtsnetweight" name="txtsnetweight" class="form-control" maxlength="100" placeholder="" readonly>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                            <label class="control-label col-lg-4 no-padding-right" for="txtgrossweight">GrossWeight<span class="text-danger"></span>:</label>

                                            <div class="col-lg-8">
                                                <div class="clearfix">
                                                    <input style="text-transform:uppercase" type="text" id="txtsgrossweight" name="txtsgrossweight" class="form-control" maxlength="100" placeholder="" readonly>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                            <label class="control-label col-lg-4 no-padding-right" for="txtquantity">Quantity<span class="text-danger"></span>:</label>

                                            <div class="col-lg-8">
                                                <div class="clearfix">
                                                    <input style="text-transform:uppercase" type="text" id="txtsquantity" name="txtsquantity" class="form-control" maxlength="100" placeholder="" readonly>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                            <label class="control-label col-lg-4 no-padding-right" for="txtactua;">Actual<span class="text-danger">*</span>:</label>

                                            <div class="col-lg-8">
                                                <div class="clearfix">
                                                    <input style="text-transform:uppercase" type="text" id="txtactua" name="txtactua" class="form-control" maxlength="100">
                                                </div>
                                            </div>
                                        </div>

                                        <div class="wizard-actions" style="text-align: right !important;margin-right: 70px;">

                                            @*<button class="btn btn-success btn-sm btn-next" id="btnadd" type="submit">

                                                    <i class="ace-icon fa fa-plus"></i>
                                                </button>*@
                                            <div class="panel-heading-btn">
                                                <a class="btn btn-success btn-sm btn-next" data-original-title="Add" title="Add" id="btnpopupadd"><i class="ace-icon fa fa-plus"></i> <span class="hidden-xs" id="actiontypepopupaddd"></span></a>
                                                <a href="javascript:;" class="btn btn-danger btn-sm" data-original-title="Clear" title="Clear" id="btnpopupitemclear"><i class="fa fa-trash"></i> <span class="hidden-xs"></span></a>
                                            </div>



                                        </div>

                                    </div>
                                </form>
                                <div class="scroll-x">
                                    <table id="dynamic-popuptableDetails" class="table table-striped table-bordered table-hover">
                                        <thead>
                                            <tr></tr>
                                        </thead>
                                        <tbody id="tbody"></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>


                        <div class="panel-heading-btn">
                            <button class="btn btn-success btn-sm btn-next" id="btnok" type="submit">
                                <span id="actiontype">OK</span>
                                <i class="ace-icon fa fa-check icon-on-center"></i>
                            </button>
                        </div>


                    </div>
                </div>
            </div>
            <div class="modal-footer">

            </div>
        </div>
    </div>
</div>


<style>
    #dynamic-printtableDetailsfinal table, #dynamic-printtableDetailsfinal td, #dynamic-printtableDetailsfinal th {
        border: 1px solid black;
        text-align: center;
    }

    #printable * {
        color: #000;
        font-size: 7px;
        font-family: Arial, sans-serif;
    }



    div#printable {
        width: 2.65in;
        height: 2.00in;
    }

    #printable table {
        border-collapse: collapse;
    }

    #printable table, #printable td, #printable th {
        border: 1px solid black;
        padding: 4px;
        text-align: center;
    }
</style>

<style>
    #printableres * {
        color: #000;
        font-size: 7px;
        font-family: Arial, sans-serif;
    }



    div#printableres {
        width: 2.65in;
        height: 2.00in;
    }

    #printableres table {
        border-collapse: collapse;
    }

    #printableres table, #printableres td, #printableres th {
        border: 1px solid black;
        padding: 4px;
        text-align: center;
        margin: 10px;
    }
</style>

<div style="visibility:hidden;overflow:auto;max-height:0px;max-width:0px;">
    <div id="printable" style="border:1px solid #000;">


    </div>
</div>


