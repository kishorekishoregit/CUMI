                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <input style="text-transform:uppercase" type="text" id="txtremarks" name="txtremarks" class="form-control" maxlength="100">
                                            </div>
                                        </div>
                                    </div>





                                </div>

                                <div class="row">
                                    @*<div class="form-group col-lg-3 col-md-3 col-sm-3">
                                        <label class="control-label col-lg-4 no-padding-right" for="txtoutquantity">Outer Quantity:</label>

                                        <div class="col-lg-8">
                                            <div class="clearfix">
