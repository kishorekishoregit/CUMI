@{
    ViewBag.Title = "SRNUpload";
    
}

<div class="page-content">
    <div class="row">
        <div class="col-xs-12 clearfix">
            <div class="widget-box1">
                
                    <div class="widget-header widget-header-blue widget-header-flat">
                        <h4 class="widget-title lighter">SRN Upload</h4>
                    </div>
                    <div class="widget-body">
                        <div class="widget-main">
                            <div id="fuelux-wizard-container" class="no-steps-container">
                                <form class="form-horizontal clearfix" action="@Url.Content("~/Transaction/SRNUpload")">
                                    <div class="row">
                                        <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                            <label class="control-label col-lg-4 no-padding-right" for="txtsrnno">SRN NO<span class="text-danger">*</span>:</label>

                                            <div class="col-lg-8">
                                                <div class="clearfix">
                                                    <input type="text" style="text-transform:uppercase" id="txtsrnno" name="txtsrnno" class="form-control">
                                                </div>
                                            </div>
                                        </div>

                                        <div class="wizard-actions" style="text-align: right !important;margin-right: 70px;">




                                            <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                                <div class="col-lg-8">

                                                    <input type="file" id="hf" name="postedFile" accept=".xlsx" autofocus style="background: #5fd6a6;color: white;margin-top: 7px;" />
                                                </div>
                                                <div class="control-label col-lg-4 no-padding-right" style="margin-top: 2px;">

                                                    <a class="btn btn-success btn-sm btn-next" data-original-title="Upload" title="Upload" id="btnupload"> <span class="hidden-xs" id="actiontypeaddd">Upload</span></a>
                                                </div>
                                            </div>
                                            <div class="form-group col-lg-4 col-md-4 col-sm-4">

                                            </div>
                                        </div>
                                    </div>
                                </form>
                                    <div class="wizard-actions" style="text-align: center !important;margin-top: 30px;">

                                        <button class="btn btn-success btn-sm btn-next " id="btnsubmit" type="submit">
                                            <span id="actiontype">Save</span>
                                            <i class="ace-icon fa fa-check icon-on-right"></i>
                                        </button>
                                        <button class="btn btn-danger btn-sm" type="submit" id="btnclear">
                                            <span id="actiontype">Clear</span>
                                            <i class="ace-icon fa fa-refresh icon-on-right"></i>
                                        </button>



                                    </div>
                                        
                                    
                               
                                    </div>
                            <div class="scroll-x" style="margin-top: 15px;">
                                <table id="dynamic-table-details" class="table table-striped table-bordered table-hover">
                                    <thead>
                                        <tr></tr>
                                    </thead>
                                    <tbody id="tbody"></tbody>
                                </table>
                            </div>

                                


                            </div>
                           
                        </div><!-- /.widget-main -->
                    </div><!-- /.widget-body -->
                </div>
        </div>
        
    </div>



<!-- /.row -->
