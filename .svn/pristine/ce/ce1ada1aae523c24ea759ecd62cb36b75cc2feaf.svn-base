            </tr>
            <tr style="height: 0.23in;">
                <td style="width:2in;" colspan="2">LOT NO.: $StuffingLotNo$</td>
                <td style="width:1in;" colspan="2">EXP DATE: $ExpiryDate$</td>
            </tr>
            <tr style="width: 100%;height: 0.23in;">
                <td colspan="2" style="width:2in;">SUPPLIER LOT NO: $LotNo$ </td>
                <td style="width:.75in;">UOM: $UOM$ </td>
                <td style="width:.75in;">$CaseNo$</td>
            </tr>

        </tbody>
    </table>

