<div style="page-break-after:always; vertical-align:bottom; ">
    <table style="width: 100%;">

        <tbody>
            <tr>
                <td style="width:1.65in;padding:0px !important;">
                    <table style="width: 100%; border:0px;padding:0px !important;">
                        <tbody>
                            <tr style="height: 0.23in;">
                                <td colspan="4" style="width:0.5in;">PART NO: $PartNo$</td>
                                <td colspan="1" style="width:0.5in;">QTY : $PartQty$</td>
                            </tr>
                            <tr style="height: 0.23in;">
                                <td colspan="4" style="width:0.5in;">DESC: $PartDescription$</td>
                            </tr>
                            <tr style="height: 0.23in;">
                                <td colspan="4" style="width:0.5in;">GRN NO. : $GRNNO$</td>
                                <td colspan="1" style="width:0.5in;">DT: $GRNDate$ </td>
                            </tr>
                            <tr style="height: 0.23in;">
                                <td colspan="4" style="width:0.5in;">MFG. NAME: $ManufactureName$ </td>
                            </tr>
                        </tbody>
                    </table>
                </td>
                <td style="width:1in;">
                    <table style="width: 100%; border:0px">
                        <tr>
                            <td style="width:1in;border: 0px !important;padding: 0px; height:0.80in;" align="center"><div id="$qrcode$"></div></td>
                        </tr>
                    </table>
                </td>

            </tr>
        </tbody>
    </table>
    <table style="width: 100%;" class="table2">
        <style>
            .table2 td {
                padding: 1px;
            }
        </style>
        <tbody>
            <tr style="height: 0.23in;">
