    //-------------------------------------------------------------------------------------------Assign values table into TextBox (Controls)   
    //get the Role Creation Details by ID
    function GetRackDetailsByID(rackcode) {
        $(".loader").show("slow");
        $.ajax({
            url: 'GetRackdetailsByID',
            dataType: 'json',
            type: 'POST',
            data: { "RACKCODE": rackcode },

            success: function (data) {
                var resJ = JSON.parse(data);
                $.each(resJ, function (i, item) {

                    $("#RackCode").val(resJ[i].RACKCODE);
                    $("#RackName").val(resJ[i].RACKNAME);
                    $('#LocationName').val(resJ[i].LOCATIONCODE)
                    $('#LocationName').trigger("chosen:updated");
                    $('#RecordStatus').val(resJ[i].RECORDSTATUS)
                    $('#RecordStatus').trigger("chosen:updated");
                });
                $("#RackCode").prop('readonly', true);
                $("#actiontype").text("Update");
                $("html, body").animate({ scrollTop: 0 }, "slow");
            }

        });
        $(".loader").hide("slow");
    }
