@{
    ViewBag.Title = "UserCreation";
}
<div class="page-content">
    <div class="row">
        <div class="col-xs-12 clearfix">
            <div class="widget-box1">
                <div class="widget-header widget-header-blue widget-header-flat">
                        <h4 class="widget-title lighter">User Creation</h4>
                    </div>
                <div class="widget-body">
                    <div class="widget-main">
                        <div id="fuelux-wizard-container" class="no-steps-container">
                            
                            <form class="form-horizontal clearfix" id="validation-form" name="myform" action="@Url.Content("~/User/UserCreation")" method="post">
                               
                                <div class="row">
                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="ddlemployeecode">Employee Code<span class="text-danger">*</span>:</label>

                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <select class="chosen-select form-control" id="ddlemployeecode" name="ddlemployeecode" data-placeholder=""></select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="txtemployeeame">Employee Name<span class="text-danger">*</span>:</label>
                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <input style="text-transform:uppercase" type="text" id="txtemployeeame" name="txtemployeeame" class="form-control" maxlength="100">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="txtusername">User Name<span class="text-danger">*</span>:</label>

                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <input style="text-transform:uppercase" type="text" id="txtusername" name="txtusername" class="form-control" maxlength="100">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="txtpassword">Password<span class="text-danger">*</span>:</label>

                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <input style="text-transform:uppercase" type="text" id="txtpassword" name="txtpassword" class="form-control" maxlength="100">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="txtconfirmpassword">Confirm Password<span class="text-danger">*</span>:</label>
                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <input style="text-transform:uppercase" type="text" id="txtconfirmpassword" name="txtconfirmpassword" class="form-control" maxlength="100">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="ddlstatus">Process Name<span class="text-danger">*</span>:</label>

                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <select class="chosen-select form-control" id="ddlprocessname" name="ddlprocessname" data-placeholder=""></select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="ddlstatus">User Status<span class="text-danger">*</span>:</label>

                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <select class="chosen-select form-control" id="ddlstatus" name="ddlstatus" data-placeholder=""></select>
                                            </div>
                                        </div>
                                    </div>

                                </div>

                            </form>
                        </div>
                        <div class="wizard-actions" style="text-align: center !important;">

                            <button class="btn btn-success btn-sm btn-next" id="btnsubmit" type="submit">
                                <span id="actiontype">Save</span>
                                <i class="ace-icon fa fa-check icon-on-right"></i>
                            </button>
                            <button class="btn btn-danger btn-sm" type="submit" id="btnclear">
                                <span id="actiontype">Clear</span>
                                <i class="ace-icon fa fa-refresh icon-on-right"></i>
                            </button>



                        </div>
                    </div><!-- /.widget-main -->
                </div><!-- /.widget-body -->
            </div>
        </div>
       
        <div class="col-xs-12 clearfix">

            <!-- div.table-responsive -->
            <div class="clearfix">
                <div class="tableTools-container"></div>
            </div>
            <div class="widget-box1">
               
                    <table id="dynamic-table" class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr></tr>
                        </thead>
                        <tbody id="tbody" class="checkuploadbody"></tbody>
                    </table>
               
                <style>
                    #dynamic-tableUpload td:empty {
                        display: none;
                    }
                </style>
            </div>
        </div>


     
        </div>
</div><!-- /.row -->
@section Scripts
{
    <script src="@Url.Content("~/Scripts/wizard.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery-additional-methods.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootbox.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.maskedinput.min.js")"></script>
    <script src="@Url.Content("~/Scripts/select2.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.dataTables.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.dataTables.bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/dataTables.buttons.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.flash.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.html5.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.print.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.colVis.min.js")"></script>
    <script src="@Url.Content("~/Scripts/dataTables.select.min.js")"></script>
    <script src="@Url.Content("~/Scripts/chosen.jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/spinbox.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-datepicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-timepicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/moment.min.js")"></script>
    <script src="@Url.Content("~/Scripts/daterangepicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-datetimepicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-colorpicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.knob.min.js")"></script>
    <script src="@Url.Content("~/Scripts/autosize.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.inputlimiter.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.maskedinput.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-tag.min.js")"></script>
    <script src="@Url.Content("~/Scripts/dataTables.select.min.js")"></script>
    <script src="@Url.Content("~/Scripts/NewFolder1/dataTables.tableTools.js")"></script>
    <script src="@Url.Content("~/FuctionScripts/UserCreation.js")"></script>
}




