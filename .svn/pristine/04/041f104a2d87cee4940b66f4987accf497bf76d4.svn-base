@{
    ViewBag.Title = "RemixQuality";
}

<div class="page-content">
    <div class="row">
        <div class="col-xs-12 clearfix">
            <div class="widget-box1">
                <div class="widget-header widget-header-blue widget-header-flat">
                    <h4 class="widget-title lighter">REMIX QUALITY</h4>
                </div>
                <div class="widget-body">
                    <div class="widget-main">
                        <div id="fuelux-wizard-container" class="no-steps-container">
                            <form class="form-horizontal clearfix" action="@Url.Content("~/Transaction/RemixQuality")">
                                <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                    <label class="control-label col-lg-4 no-padding-right" for="RecordStatus">LOT NO<span class="text-danger">*</span>:</label>

                                    <div class="col-lg-8">
                                        <div class="clearfix">
                                            <select class="chosen-select form-control" id="ddllotno" name="ddllotno" data-placeholder=""></select>
                                        </div>
                                    </div>
                                </div>



                            </form>



                        </div>
                        <div class="wizard-actions" style="text-align: center !important;">

                            <button class="btn btn-success btn-sm btn-next" id="btnprint" type="submit">
                                <span id="actiontype">Approve</span>
                                <i class="ace-icon fa fa-check icon-on-right"></i>
                            </button>




                        </div>
                    </div><!-- /.widget-main -->
                </div><!-- /.widget-body -->
            </div>
        </div>


    </div>
</div><!-- /.row -->



<style>
    #dynamic-printtableDetailsfinal table, #dynamic-printtableDetailsfinal td, #dynamic-printtableDetailsfinal th {
        border: 1px solid black;
        text-align: center;
    }

    #printable * {
        color: #000;
        font-size: 7px;
        font-family: Arial, sans-serif;
    }



    div#printable {
        width: 2.65in;
        height: 2.00in;
    }

    #printable table {
        border-collapse: collapse;
    }

    #printable table, #printable td, #printable th {
        border: 1px solid black;
        padding: 4px;
        text-align: center;
    }
</style>

<style>
    #printableres * {
        color: #000;
        font-size: 7px;
        font-family: Arial, sans-serif;
    }



    div#printableres {
        width: 2.65in;
        height: 2.00in;
    }

    #printableres table {
        border-collapse: collapse;
    }

    #printableres table, #printableres td, #printableres th {
        border: 1px solid black;
        padding: 4px;
        text-align: center;
        margin: 10px;
    }
</style>

<div style="visibility:hidden;overflow:auto;max-height:0px;max-width:0px;">
    <div id="printable" style="border:1px solid #000;">


    </div>
</div>
