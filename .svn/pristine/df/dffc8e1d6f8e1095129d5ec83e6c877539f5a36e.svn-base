<div style="page-break-after:always; vertical-align:bottom;">
    <table style="width: 100%">
        <tr>
            <td>PART NO:<span style="font-size:12px;font-weight:600;">$PartNo$</span></td>
            <td>QTY : $PartQty$</td>
            <td rowspan="4" colspan="3" align="center">
                <div id="$qrcode$"></div>
            </td>
        </tr>
        <tr>
            <td colspan="2">DESC: $PartDescription$</td>
        </tr>
        <tr>
            <td>GRN NO. :<span style="font-size:12px;font-weight:600;"> $GRNNO$</span></td>
            <td>DT: $GRNDate$</td>
        </tr>
        <tr>
            <td colspan="2">MFG. NAME: <span style="font-size:12px;font-weight:600;">$ManufactureName$</span></td>
        </tr>
        <tr>
            <td colspan="1">MFG. P/N: $ManufacturePartNo$</td>
            <td colspan="3">MFG DATE: $ManufactureDate$</td>
        </tr>
        <tr>
            <td>LOT NO.:<span style="font-size:12px;font-weight:600;"> $StuffingLotNo$</span></td>
            <td colspan="2">EXP DATE: $ExpiryDate$</td>
        </tr>
        <tr>
            <td>Sup. LOT No: $LotNo$</td>
            <td>UOM:$UOM$</td>
            <td>$CaseNo$</td>
        </tr>
    </table>
</div>




