@{
    ViewBag.Title = "GRNUpload";
}

<div class="page-content">
    <div class="row">
        <div class="col-xs-12 clearfix">
            <div class="widget-box1">

                <div class="widget-body">
                    <div class="widget-main">
                        <div id="fuelux-wizard-container" class="no-steps-container">
                            <form class="form-horizontal clearfix" action="@Url.Content("~/Transaction/GRNUpload")">
                                <div class="row">
                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="txtgrnno">GRN No.<span class="text-danger">*</span>:</label>

                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <input type="text" style="text-transform:uppercase" id="txtgrnno" name="txtgrnno" class="form-control" autofocus maxlength="20">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="txtgrndate">GRN Date<span class="text-danger">*</span>:</label>
                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <input style="text-transform:uppercase" type="text" id="txtgrndate" name="txtgrndate" class="form-control" maxlength="100" tabindex="1">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="ddlsupplier">Supplier<span class="text-danger">*</span>:</label>

                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <select class="chosen-select form-control" id="ddlsupplier" name="ddlsupplier" data-placeholder="" tabindex="2"></select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">


                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="txtreferenceno">Reference No.:</label>
                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <input style="text-transform:uppercase" type="text" id="txtreferenceno" name="txtreferenceno" class="form-control" maxlength="100" tabindex="3">
                                            </div>
                                        </div>
                                    </div>

                                </div>

                                
                              
                              

                                <div class="wizard-actions" style="text-align: right !important;margin-right: 70px;">

                                
                                        <div class="row">
                                          
                                                <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                                    <div class="col-lg-8">

                                                        <input type="file" id="hf" name="postedFile" accept=".xlsx" autofocus style="background: #5fd6a6;color: white;" />
                                                    </div>
                                                    <div class="col-lg-8" style="margin-top: 10px;">

                                                        <a class="btn btn-success btn-sm btn-next" data-original-title="Upload" title="Upload" id="btnupload"> <span class="hidden-xs" id="actiontypeaddd">Upload</span></a>
                                                    </div>
                                                </div>
                                                <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                                   
                                                </div>
                                            </div>
                                        
                                    
                                    @*<button class="btn btn-success btn-sm btn-next" id="btnadd" type="submit">
                                            <span class="hidden-xs" id="actiontypeaddd"></span>
                                            <i class="ace-icon fa fa-plus"></i>
                                        </button>
                                        <button class="btn btn-danger btn-sm" type="submit" id="btnclear">

                                            <i class="ace-icon fa fa-trash"></i>
                                        </button>*@

