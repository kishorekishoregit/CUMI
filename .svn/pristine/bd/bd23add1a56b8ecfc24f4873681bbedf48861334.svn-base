@{
    ViewBag.Title = "Profile";
}
<div class="page-content">
    <div class="row">
        <div class="col-xs-12">
            <div id="user-profile-2" class="user-profile">
                <div class="tabbable">
                    <ul class="nav nav-tabs padding-18">
                        <li class="active">
                            <a data-toggle="tab" href="#home">
                                <i class="green ace-icon fa fa-user bigger-120"></i>
                                Profile
                            </a>
                        </li>
                        <li>
                            <a data-toggle="tab" href="#feed">
                                <i class="orange ace-icon fa fa-key bigger-120"></i>
                                Change Password
                            </a>
                        </li>
                    </ul>
                    <div class="tab-content no-border padding-24">
                        <div id="home" class="tab-pane in active">
                            <div class="row">
                                <div class="col-xs-12 col-sm-6">
                                    <h4 class="blue">
                                        <span class="middle">@ViewBag.LoginUser</span>
                                        <span class="label label-purple arrowed-in-right">
                                            <i class="ace-icon fa fa-circle smaller-80 align-middle"></i>
                                            active
                                        </span>
                                    </h4>
                                    <div class="profile-user-info">
                                        <div class="profile-info-row">
                                            <div class="profile-info-name"> <b>Code :</b> </div>
                                            <div class="profile-info-value">
                                                <span class="label label-info" id="vendorcode"></span>
                                            </div>
                                        </div>
                                        <div class="profile-info-row">
                                            <div class="profile-info-name"> <b>Name :</b> </div>
                                            <div class="profile-info-value">
                                                <span class="label label-info" id="vendorname"></span>
                                            </div>
                                        </div>
                                        <div class="profile-info-row">
                                            <div class="profile-info-name"><b>Mobile No:</b>  </div>
                                            <div class="profile-info-value">
                                                <span class="label label-info" id="pn"></span>
                                            </div>
                                        </div>
                                        @*<div class="profile-info-row">
                                                <div class="profile-info-name"><b>Website :</b>  </div>
                                                <div class="profile-info-value">
                                                    <span class="label label-info" id="website"></span>
                                                </div>
                                            </div>*@
                                        <div class="profile-info-row">
                                            <div class="profile-info-name"><b>Address :</b>  </div>
                                            <div class="profile-info-value">
                                                <span class="label label-info" id="address"></span>
                                            </div>
                                        </div>
                                        <div class="profile-info-row">
                                            <div class="profile-info-name"><b>Email :</b>  </div>
                                            <div class="profile-info-value">
                                                <span class="label label-info" id="email"></span>
                                            </div>
                                        </div>
                                        <div class="profile-info-row">
                                            <div class="profile-info-name"><b>Postal Code :</b>  </div>
                                            <div class="profile-info-value">
                                                <span class="label label-info" id="postalcode"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div><!-- /.col -->
                            </div><!-- /.row -->
                        </div><!-- /#home -->
                        <div id="feed" class="tab-pane">
                            <div class="row">
                                <div class="col-xs-12 col-sm-5">
                                    <h4 class="blue">
                                        <span class="middle">Change Password</span>
                                    </h4>
                                    <div class="profile-user-info">
                                        <div class="profile-info-row">
                                            <div class="profile-info-name"> <b>New Password :</b> </div>
                                            <div class="profile-info-value">
                                                <input type="password" id="newpassword" name="newpassword" class="form-control">
                                            </div>
                                        </div>
                                        <div class="profile-info-row">
                                            <div class="profile-info-name"><b>Change Password :</b>  </div>
                                            <div class="profile-info-value">
                                                <input type="password" id="changepassword" name="changepassword" class="form-control">
                                            </div>
                                        </div>
                                        <div class="profile-info-row">
                                            <div class="profile-info-name"><b>&nbsp;</b>  </div>
                                            <button class="btn btn-success btn-sm btn-next" type="button" id="btnconform">
                                                <span id="actiontype">Save</span>
                                                <i class="ace-icon fa fa-check icon-on-right"></i>
                                            </button>
                                            @*<button class="btn btn-warning btn-sm" type="submit" onclick="Clear()">
                                                    Clear
                                                    <i class="ace-icon fa fa-refresh icon-on-right"></i>
                                                </button>*@
                                        </div>
                                    </div>
                                </div>
                            </div><!-- /#feed -->
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- /.col -->
    </div><!-- /.row -->
</div>
@section Scripts
{
    <script src="@Url.Content("~/Scripts/wizard.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery-additional-methods.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootbox.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.maskedinput.min.js")"></script>
    <script src="@Url.Content("~/Scripts/select2.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.dataTables.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.dataTables.bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/dataTables.buttons.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.flash.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.html5.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.print.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.colVis.min.js")"></script>
    <script src="@Url.Content("~/Scripts/dataTables.select.min.js")"></script>
    <script src="@Url.Content("~/Scripts/chosen.jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/spinbox.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-datepicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-timepicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/moment.min.js")"></script>
    <script src="@Url.Content("~/Scripts/daterangepicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-datetimepicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-colorpicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.knob.min.js")"></script>
    <script src="@Url.Content("~/Scripts/autosize.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.inputlimiter.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.maskedinput.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-tag.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootbox.js")"></script>
    <script src="@Url.Content("~/Scripts/NewFolder1/dataTables.tableTools.js")"></script>
    <script src="@Url.Content("~/Scripts/ProfilePage.js")"></script>
}
