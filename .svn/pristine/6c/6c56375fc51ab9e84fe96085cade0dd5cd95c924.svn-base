    function Clear() {

        location.reload();

    }




    $("#btnaccept").click(function () {

        jQuery("table.checkupload tbody > tr").each(function () {

            if (jQuery('td:eq(0)', this).children('input.checkbox').is(':checked')) {

                // locationcode += jQuery('td:eq(1)', this).text() + "^";
                PRNNO += $(this).closest('tr').find('td:eq(1)').html() + "^";
                PARTNAME += $(this).closest('tr').find('td:eq(2)').html() + "^";
                CUSTOMER += $(this).closest('tr').find('td:eq(3)').html() + "^";
                MODEL += $(this).closest('tr').find('td:eq(4)').html() + "^";
                VARIANT += $(this).closest('tr').find('td:eq(5)').html() + "^";
                LOTNO += $(this).closest('tr').find('td:eq(6)').html() + "^";
                REJECTEDQUANTITY = "0";
                QUANTITY += $(this).closest('tr').find('td:eq(7)').html() + "^";
               
                REMARKS = '';
                LOTSTATUS = "ACCEPT";
            }

        });

        $(".loader").show("slow");

        $.ajax({
            url: 'InsertQCEntry',
            dataType: "json",
            type: "POST",
            data: $.param({ 'PRNNO': PRNNO })
                + '&' + $.param({ 'PARTNAME': PARTNAME })
                + '&' + $.param({ 'CUSTOMER': CUSTOMER })
                + '&' + $.param({ 'MODEL': MODEL })
                + '&' + $.param({ 'VARIANT': VARIANT })
                + '&' + $.param({ 'LOTNO': LOTNO })
                + '&' + $.param({ 'REJECTEDQUANTITY': REJECTEDQUANTITY })
                + '&' + $.param({ 'QUANTITY': QUANTITY })
                + '&' + $.param({ 'REMARKS': REMARKS })
                + '&' + $.param({ 'LOTSTATUS': LOTSTATUS }),
            //  data:formData,
            success: function (data) {

                var Res = data.split('|');
                var result = Res[0];
                var msg = Res[1];
                if (result.toUpperCase() == "TRUE") {
                    // $("form").trigger("reset");

                    $('.loadingGIF').hide();
                    sweetAlert("", msg.replace(/&quot;/g, "").replace(/<Br \/>/g, "\n"), "success");
                    Clear();
                }
                else {
                    $('.loadingGIF').hide();
                    sweetAlert("", msg.replace(/&quot;/g, "").replace(/<Br \/>/g, "\n"), "error");
                   
                }
            }
        });

        $(".loader").hide("slow");

    });


    $("#btnreject").click(function () {

        jQuery("table.checkupload tbody > tr").each(function () {

            if (jQuery('td:eq(0)', this).children('input.checkbox').is(':checked')) {


                PRNNO += $(this).closest('tr').find('td:eq(1)').html() + "^";
