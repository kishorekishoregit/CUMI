               // REMARKS = '';
              
                LOTSTATUS = "REJECT";


            }

        });

        $('#modalview').modal('show');
    });

    $('#checkall').click(function (event) {
        if (this.checked) {
            // Iterate each checkbox
            $(':checkbox').each(function () {
                this.checked = true;
            });
        } else {
            $(':checkbox').each(function () {
                this.checked = false;
            });
