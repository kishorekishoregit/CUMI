@{
    ViewBag.Title = "Pasting Process";
}


<div class="page-content">
    <div class="row">
        <div class="col-xs-12 clearfix">
            <div class="widget-box1">
                <div class="widget-header widget-header-blue widget-header-flat">
                    <h4 class="widget-title lighter">Pasting Process</h4>
                </div>
                <div class="widget-body">
                    <div class="widget-main">
                        <div id="fuelux-wizard-container" class="no-steps-container">
                            <form class="form-horizontal clearfix" action="@Url.Content("~/Transaction/PastingProcess")">
                                <div class="row">
                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="txtbarcode">
                                            Barcode <span class="text-danger">*</span>:
                                        </label>

                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <input type="text" style="text-transform:uppercase" id="txtbarcode" name="txtbarcode" class="form-control" autofocus maxlength="20">
                                                <input type="hidden" id="hdautoid">
                                            </div>
                                        </div>
                                    </div>
                  
                                </div>

                             
                            

                            </form>

              



                            <div class="hr hr-18 hr-double dotted col-lg-12"></div>
                            <div class="col-xs-12 clearfix">
                                <h3 class="header smaller lighter blue">Pasting Barcode Details</h3>
                                <!-- div.table-responsive -->
                                <div class="clearfix">
                                    <div class="tableTools-container"></div>
                                </div>
                                <div class="scroll-x">
                                    <table id="dynamic-table" class="table table-striped table-bordered table-hover checkupload">
                                        <thead>
                                            <tr></tr>
                                        </thead>
                                        <tbody id="tbody" class="checkuploadbody"></tbody>
                                    </table>
                                </div>
                                <style>
                                    #dynamic-tableUpload td:empty {
                                        display: none;
                                    }
                                </style>
                            </div>

                            <div class="wizard-actions" style="text-align: center !important;margin-top:20px">

                                <button class="btn btn-success btn-sm btn-next" id="btnsubmit" type="submit">
                                    <span id="actiontype">Save & Print </span>
                                    <i class="ace-icon fa fa-check icon-on-right"></i>
                                </button>
                                <button class="btn btn-danger btn-sm" type="submit" id="btnclear">
                                    <span id="actiontype">Clear</span>
                                    <i class="ace-icon fa fa-refresh icon-on-right"></i>
                                </button>



                            </div>


    </div>
</div><!-- /.row -->
@*POPUP*@




<style>
    #dynamic-printtableDetailsfinal table, #dynamic-printtableDetailsfinal td, #dynamic-printtableDetailsfinal th {
        border: 1px solid black;
        text-align: center;
    }

    #printable * {
        color: #000;
        font-size: 7px;
        font-family: Arial, sans-serif;
    }



    div#printable {
        width: 2.65in;
        height: 2.00in;
    }

    #printable table {
        border-collapse: collapse;
    }

    #printable table, #printable td, #printable th {
        border: 1px solid black;
        padding: 4px;
        text-align: center;
    }
</style>

<style>
    #printableres * {
        color: #000;
        font-size: 7px;
        font-family: Arial, sans-serif;
    }



    div#printableres {
        width: 2.65in;
        height: 2.00in;
    }

    #printableres table {
        border-collapse: collapse;
    }

    #printableres table, #printableres td, #printableres th {
        border: 1px solid black;
        padding: 4px;
        text-align: center;
        margin: 10px;
    }
</style>

<div style="visibility:hidden;overflow:auto;max-height:0px;max-width:0px;">
    <div id="printable" style="border:1px solid #000;">


    </div>
</div>


