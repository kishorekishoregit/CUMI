@{
    ViewBag.Title = "RemixJobProcess";
}
<div class="page-content">
    <div class="row">
        <div class="col-xs-12 clearfix">
            <div class="widget-box1">
                <div class="widget-header widget-header-blue widget-header-flat">
                    <h4 class="widget-title lighter">REMIX Job Process</h4>
                </div>
                <div class="widget-body">
                    <div class="widget-main">
                        <div id="fuelux-wizard-container" class="no-steps-container">
                            <form class="form-horizontal clearfix" action="@Url.Content("~/Transaction/RemixJobProcess")">
                                <div class="row">
                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="txtjobprocessno">Job Process No.<span class="text-danger">*</span>:</label>

                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <input type="text" style="text-transform:uppercase" id="txtjobprocessno" name="txtjobprocessno" class="form-control" autofocus maxlength="20">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="txtjobprocessdate">Job Process Date<span class="text-danger">*</span>:</label>
                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <input style="text-transform:uppercase" type="text" id="txtjobprocessdate" name="date" class="form-control" maxlength="100" tabindex="1">
                                            </div>
                                        </div>
                                    </div>
                             
                                </div>
                                <div class="row">

                                </div>

                                <div class="row">
                                    <h4 style="background-color: #e3e3e3;padding-left: 13px;padding-top: 7px;padding-bottom: 7px;"></h4>
                                </div>
                                <div class="row" style="margin-top: 15px;">
                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="ddlitemcode">Item Code<span class="text-danger">*</span>:</label>

                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <select class="chosen-select form-control" id="ddlitemcode" name="ddlitemcode" data-placeholder=""></select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="txtitemname">Item Name<span class="text-danger">*</span>:</label>
                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <input style="text-transform:uppercase" type="text" id="txtitemname" name="txtitemname" class="form-control" maxlength="100" readonly>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="txtweight">Weight<span class="text-danger">*</span>:</label>
                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <input style="text-transform:uppercase" type="text" id="txtweight" name="txtweight" class="form-control" maxlength="100">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                              

                                   
                                   
                                </div>
                                <div class="row">
                                   

                                </div>

                                <div class="wizard-actions" style="text-align: right !important;margin-right: 70px;">

                                    <div class="panel-heading-btn">
                                        <a class="btn btn-success btn-sm btn-next" data-original-title="Add" title="Add" id="btnadd"><i class="ace-icon fa fa-plus"></i> <span class="hidden-xs" id="actiontypeaddd"></span></a>
                                        <a href="javascript:;" class="btn btn-danger btn-sm" data-original-title="Clear" title="Clear" id="btnitemclear"><i class="fa fa-trash"></i> <span class="hidden-xs"></span></a>
                                    </div>
                                    @*<button class="btn btn-success btn-sm btn-next" id="btnadd" type="submit">
                                            <span class="hidden-xs" id="actiontypeaddd"></span>
                                            <i class="ace-icon fa fa-plus"></i>
                                        </button>
                                        <button class="btn btn-danger btn-sm" type="submit" id="btnclear">

                                            <i class="ace-icon fa fa-trash"></i>
                                        </button>*@



                                </div>
                                <div class="scroll-x">
                                    <table id="dynamic-tableChild" class="table table-striped table-bordered table-hover">
                                        <thead>
                                            <tr></tr>
                                        </thead>
                                        <tbody id="tbody"></tbody>
                                    </table>
                                </div>

                            </form>



                        </div>
                        <div class="wizard-actions" style="text-align: center !important;margin-top: 30px;">

                            <button class="btn btn-success btn-sm btn-next" id="btnsubmit" type="submit">
                                <span id="actiontype">Save</span>
                                <i class="ace-icon fa fa-check icon-on-right"></i>
                            </button>
                            <button class="btn btn-danger btn-sm" type="submit" id="btnclear">
                                <span id="actiontype">Clear</span>
                                <i class="ace-icon fa fa-refresh icon-on-right"></i>
                            </button>



                        </div>
                    </div><!-- /.widget-main -->
                </div><!-- /.widget-body -->
            </div>
        </div>
       
        <div class="col-xs-12 clearfix">
      
            <div class="clearfix">
                <div class="tableTools-container"></div>
            </div>
            <div class="widget-box1">
                <table id="dynamic-table" class="table table-striped table-bordered table-hover">
                    <thead>
                        <tr></tr>
                    </thead>
                    <tbody id="tbody" class="checkuploadbody"></tbody>
                </table>
            </div>
            <style>
                #dynamic-tableUpload td:empty {
                    display: none;
                }
            </style>
        </div>
    </div>

    <div class="modal" id="modalview">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="popup_header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h3 class="header smaller lighter blue">Job Process Details</h3>

                </div>
                <div class="modal-body">
                    <div class="tabbable">


                        <div class="tab-content no-border padding-24">

                            <div id="upload" class="tab-pane active">
                                <div id="fuelux-wizard-container" class="no-steps-container">
                                    <div class="col-xs-12 clearfix">
                                        <div class="table-responsive">
                                            <table id="dynamic-tableDetails" class="table table-striped table-bordered table-hover">
                                                <thead>
                                                    <tr></tr>
                                                </thead>
                                                <tbody id="tbody"></tbody>

                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>



                        </div>
                    </div>
                </div>
                <div class="modal-footer">

                </div>
            </div>
        </div>
    </div>

</div><!-- /.row -->
