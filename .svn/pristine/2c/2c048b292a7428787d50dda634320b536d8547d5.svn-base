<div style="page-break-after:always; vertical-align:bottom;">
    <table style="width: 100%">
        <tr>
	    <td colspan="3" style="vertical-align:middle; height:0.1in;max-height:0.1in;font-weight:600;">PART NO:<span style="font-size:13px !important;font-weight:900;">$PartNo$</span></td>
            <td style="width:0.70in !important;font-weight:600;">QTY : <span style="font-weight:600;">$StuffingQty$</span></td>
        </tr>
        <tr>
             <td colspan="3" style="vertical-align:top;height:0.2in !important;max-height:0.3in;font-weight:600;">DESC: $Partname$</td>
            <td rowspan="3" align="center" class="qr-panel" style="width:0.40in !important;">
                <div id="$qrcode$"></div>
            </td>
        </tr>
       
        <tr>
            <td colspan="3" style="border-right:0px;" >Tracking Details: $TrackingDetails$ </td>
            <!--<td style="border-left:0px;">Dt: 02.02.2017</td>
            <td>Qty:23000</td>-->
        </tr>
        <tr>
            <td colspan="3">Job Order NO. :<span style="font-size:10px;"> $JobOrderNo$</span></td>
        </tr>
        <tr>
            <td colspan="4">Lot No:<span style="font-size:13px !important;font-weight:900;">$LotNo$</span>  </td>
        </tr>
        <tr>
            <td colspan="4" style="border-right:0px;" >DATE: $ProductionEntryDate$</td>
            <!--<td style="border-left:0px;">SHIFT: KG</td>
            <td>CAVITY: KG</td>-->
        </tr>
        <tr>
            <td   style="border-right:0px; border-bottom:0px;">Operator Name:</td>
            <td colspan="2"  style="border-left:0px; border-bottom:0px; border-right:0px;">In Process QA</td>
            <td  style="border-left:0px; border-bottom:0px;">Out Going QA</td>
        </tr>
        <tr>
            <td  style="border-right:0px; border-top:0px;">$OperatorName$</td>
            <td colspan="2"  style="border-left:0px; border-right:0px; border-top:0px;"></td>
            <td style="border-left:0px; border-top:0px;"></td>
        </tr>
        <tr>
            <!--<td colspan="3">LOT NO:<span style ="font-size:15px;  font-family:serif;"><b>S13317000551-002</b></span> </td>-->
            <td colspan="3">UOM:$UOM$</td>
            <td colspan="1">$CaseNo$</td>
        </tr>
    </table>
</div>




