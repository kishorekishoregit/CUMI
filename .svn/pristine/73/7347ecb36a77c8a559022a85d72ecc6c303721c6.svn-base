
@{
    ViewBag.Title = "RMPicklistPrint";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="page-content">
    <div class="row">
        <div class="col-xs-12 clearfix">
            <div class="widget-box1">
                <div class="widget-header widget-header-blue widget-header-flat">
                    <h4 class="widget-title lighter">RM Picklist Print</h4>
                </div>
                <div class="widget-body">
                    <div class="widget-main">
                        <div id="fuelux-wizard-container" class="no-steps-container">
                            <form class="form-horizontal clearfix" action="@Url.Content("~/Report/RMPicklistPrint")">
                                <div class="row">

                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">
                                        <label class="control-label col-lg-4 no-padding-right" for="ddlsupplier">Picklist No.<span class="text-danger">*</span>:</label>

                                        <div class="col-lg-8">
                                            <div class="clearfix">
                                                <select class="chosen-select form-control" id="ddlpicklistno" name="ddlpicklistno" data-placeholder="" tabindex="2"></select>
                                            </div>
                                        </div>
                                    </div>
                                </div>



                            </form>



                        </div>
                        <div class="wizard-actions" style="text-align: center !important;margin-top: 30px;">

                            <button class="btn btn-success btn-sm btn-next" id="btngenerate" type="submit">
                                <span id="actiontype">Generate</span>
                                <i class="ace-icon fa fa-check icon-on-right"></i>
                            </button>
                            <button class="btn btn-success btn-sm btn-next" id="btnPrint" type="submit">
                                <span id="actiontype">Print</span>
                                <i class="ace-icon fa fa-check icon-on-right"></i>
                            </button>
                            <button class="btn btn-danger btn-sm" type="submit" id="btnclear">
                                <span id="actiontype">Clear</span>
                                <i class="ace-icon fa fa-refresh icon-on-right"></i>
                            </button>



                        </div>
                    </div><!-- /.widget-main -->
                </div><!-- /.widget-body -->
            </div>
        </div>

        <div class="col-sm-12">
            <div class="card">
                <div class="card-header">
                 

                </div>
                <div class="card-body table-border-style" style="margin-top: 15px;">
                    <div style="background-color:white;margin-left: 10px;margin-right: 10px;" id="dvContents">
                        <style>
                            table, td, th {
                                border: 1px solid black;
                            }

                            table {
                                border-collapse: collapse;
                                width: 100%;
                            }

                            th {
                                height: 50px;
                            }

                            div.bor > table > tbody > tr > td, div.bor > table > tbody > tr > th {
                                border: 1px solid transparent !important;
                            }
                        </style>
                        <table width="100%">
                            <tbody>
                                <tr>
                                    <td style="text-align: center;height:40px;">
                                        <h2>&nbsp;<strong>RM PickList Details</strong></h2>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <table style="height: 23px;" width="100%">
                            <tbody>
                                <tr>

                                    <td>
                                        <div class="bor">
                                            <table style="height: 62px;" width="100%">
                                                <tbody>
                                                    <tr>
                                                        <th>&nbsp;Picklist No:</th>
                                                        <th id="rptpicklistno">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</th>
                                                        <th>&nbsp;Picklist Date:</th>
                                                        <th id="rptPicklistDate">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</th>

                                                    </tr>
                                                    <tr></tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <table style="height: 21px;" width="100%">
                            <thead>
                                <tr style="text-align: center;">
                                    <th style="text-align: center;" width="50px">SNO.</th>

                                    <th style="text-align: center;" width="90px">Item Code</th>
                                    <th style="text-align: center;" width="90px">Item Name</th>
                                    <th style="text-align: center;" width="90px">Lot No</th>
                                    <th style="text-align: center;" width="90px">Lot Quantity</th>
                                    <th style="text-align: center;" width="90px">Picked Quantity</th>
                                    <th style="text-align: center;" width="90px">Location</th>
                                </tr>
                            </thead>
                            <tbody id="rptgrid"></tbody>
                        </table>
                        <table width="100%">
                            <tbody>
                                <tr>

                                    <td height="55px" valign="top" style="padding: 5px;">
                                        Remarks:
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    

</div><!-- /.row -->
@section Scripts
{
    <script src="@Url.Content("~/Scripts/wizard.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery-additional-methods.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootbox.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.maskedinput.min.js")"></script>
    <script src="@Url.Content("~/Scripts/select2.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.dataTables.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.dataTables.bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Scripts/dataTables.buttons.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.flash.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.html5.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.print.min.js")"></script>
    <script src="@Url.Content("~/Scripts/buttons.colVis.min.js")"></script>
    <script src="@Url.Content("~/Scripts/dataTables.select.min.js")"></script>
    <script src="@Url.Content("~/Scripts/chosen.jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/spinbox.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-datepicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-timepicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/moment.min.js")"></script>
    <script src="@Url.Content("~/Scripts/daterangepicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-datetimepicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-colorpicker.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.knob.min.js")"></script>
    <script src="@Url.Content("~/Scripts/autosize.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.inputlimiter.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.maskedinput.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-tag.min.js")"></script>
    <script src="@Url.Content("~/Scripts/dataTables.select.min.js")"></script>
    <script src="@Url.Content("~/Scripts/NewFolder1/dataTables.tableTools.js")"></script>
    <script src="@Url.Content("~/FuctionScripts/PicklistPrint.js")"></script>
}


