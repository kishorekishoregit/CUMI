@{
    ViewBag.Title = "UserCreation";
}
<div class="row">
    <div id="fuelux-wizard-container" class="no-steps-container">

        <div class="card bd-0">

            <div class="card-body bd bd-t-0">
                <form class="form-horizontal clearfix" id="usercreation-form" name="myform" action="@Url.Content("~/User/UserCreation")" method="post">
                    <div class="card-header tx-medium bd-0 tx-white bg-indigo">
                        User Creation

                    </div><!-- card-header -->
                    <div class="row row-sm" style=" margin-top: 25px;">
                        @*<div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <div id="slWrapper1" class="parsley-select">
                                <label class="control-label" for="ddlemployeeid">Employee Code<span class="text-danger">*</span>:</label>
                                <select class="form-control select2" id="ddlemployeeid" data-parsley-class-handler="#slWrapper1" data-parsley-errors-container="#slErrorContainer1" required autofocus tabindex="1">
                                </select>
                                <div id="slErrorContainer1"></div>
                            </div>
                        </div>*@
                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="form-control-label">Employee Code<span class="text-danger">*</span>:</label>
                            <input id="ddlemployeeid" class="form-control" name="ddlemployeeid" placeholder="Enter Employee Code" type="text" tabindex="1" autofocus autocomplete="off" required>
                            <input type="hidden" id="hdautoid">
                        </div><!-- col -->

                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="form-control-label">Employee Name<span class="text-danger">*</span>:</label>
                            <input id="txtemployeename" class="form-control" name="txtemployeename" placeholder="Enter Employee Name" type="text" tabindex="2"autocomplete="off" required>
                            <input type="hidden" id="hdautoid">
                        </div><!-- col -->

                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="form-control-label">Username<span class="text-danger">*</span>:</label>
                            <input id="txtusername" class="form-control" name="txtusername" placeholder="Enter Username" required type="text" tabindex="3" autocomplete="off" maxlength="50">
                        </div><!-- col -->

                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="form-control-label">Password<span class="text-danger">*</span>:</label>
                            <input type="text" id="txtpassword" class="form-control" placeholder="Enter your Password" required tabindex="4" autocomplete="off" maxlength="50">
                        </div>

                    </div>

                    <div class="row row-sm" style=" margin-top: 25px;">

                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="form-control-label">Confirm Password <span class="tx-danger">*</span>:</label>
                            <input type="password" id="txtconfirmpassword" class="form-control" placeholder="Enter your Confirm Password" required autocomplete="off" tabindex="5" maxlength="50">
                        </div>

                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <div id="slWrapper3" class="parsley-select">
                                <label class="control-label" for="ddlstatus">Employee Status <span class="text-danger">*</span>:</label>
                                <select class="form-control select2" id="ddlstatus" data-parsley-class-handler="#slWrapper3" data-parsley-errors-container="#slErrorContainer3" required tabindex="6">
                                </select>
                                <div id="slErrorContainer3"></div>
                            </div>
                        </div>

                    </div>

                    <div class="wizard-actions" style="text-align: center !important; margin-top: 50px; margin-bottom: 20px;">

                        <button class="btn btn-az-secondary" id="btnsubmit" type="submit" tabindex="7">
                            <span id="actiontype">Save</span>
                            <i class="typcn typcn-bookmark icon-on-left"></i>
                        </button>
                        <a class="btn btn-danger btn-sm"  id="btnclear" tabindex="8">
                            <span id="actiontype">Clear</span>
                            <i class="typcn typcn-social-at-circular icon-on-left"></i>
                        </a>



                    </div>
                </form>
                <div class="card-header tx-medium bd-0 tx-white bg-indigo" style="border-radius: 5px;">
                    User Creation Details
                </div><!-- card-header -->
                <div id="dynamic-table_wrapper" class="dataTables_wrapper " style="margin-top: 20px;">

                    <table id="dynamic-table" class="table table-striped table-bordered table-hover checkupload">
                        <tbody id="tbody"></tbody>
                    </table>
                </div>
            </div>
        </div><!-- card-body -->


    </div>
</div>





