@{
    ViewBag.Title = "MoldReceiptRDC";
}

<div class="row">
    <div id="fuelux-wizard-container" class="no-steps-container">

        <div class="card bd-0">

            <div class="card-body bd bd-t-0">

                <div class="card-header tx-medium bd-0 tx-white bg-indigo">
                    Mould Receipt RDC

                </div><!-- card-header -->
                <div class="row row-sm" style=" margin-top: 25px;">

                    <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                        <div id="slWrapper2" class="parsley-select">
                            <label class="control-label" for="ddlldepartment">RDC No<span class="text-danger">*</span>:</label>
                            <select class="form-control select2" id="ddlrdcno" data-parsley-class-handler="#slWrapper2" data-parsley-errors-container="#slErrorContainer2" name="ddlrdcno" tabindex="1" required>
                            </select>
                        </div>
                        <div id="slErrorContainer2"></div>
                    </div>
                    <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                        <label class="form-control-label">RFID No <span class="tx-danger">*</span>:</label>
                        <input id="txtscan" class="form-control" name="txtscan" placeholder="Scan" type="text" autocomplete="off" maxlength="200" tabindex="2">
                    </div>
                </div>




                <hr class="mg-y-30">
                <div class="card-header tx-medium bd-0 tx-white bg-indigo" style="border-radius: 5px;">
                    Mould Receipt RDC Details
                </div>
                <div class="wrapper" style="margin-top: 30px;">
                    <table id="dynamic-table2" class="table table-striped table-bordered table-hover checkupload">
                        <thead>

                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
                <div class="wizard-actions" style="text-align: center !important; margin-top: 50px; margin-bottom: 20px;">

                    <button class="btn btn-az-secondary" id="btnsubmit" type="submit" title="Save" tabindex="7">
                        <span id="actiontype">Save</span>
                        <i class="typcn typcn-bookmark icon-on-left"></i>
                    </button>
                    <a class="btn btn-danger btn-sm" id="btnclear" title="Clear" tabindex="8">
                        <span id="actiontype">Clear</span>
                        <i class="typcn typcn-social-at-circular icon-on-left"></i>
                    </a>


                </div>

                <div class="card-header tx-medium bd-0 tx-white bg-indigo" style="border-radius: 5px;">
                    Mould Receipt RDC
                </div><!-- card-header -->

                <div id="dynamic-table_wrapper" class="dataTables_wrapper no-footer" style="margin-top: 20px;">
                    <table id="dynamic-table" class="table table-striped table-bordered table-hover">
                    </table>
                </div>


            </div>
        </div>
    </div><!-- card-body -->


</div>

<div class="loadingGIF" align="center">
    
    <img src="~/Images/jump_logo.gif" alt=""  />
</div>
<link href="~/Content/AjaxLoader.css" rel="stylesheet" />


