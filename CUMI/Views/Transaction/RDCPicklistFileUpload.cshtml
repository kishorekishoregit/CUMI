@{
    ViewBag.Title = "RDCPicklistFileUpload";
}

<div class="page-content">
    <div class="row">
        <div class="col-xs-12 clearfix">
            <div class="widget-box1">

                <div class="card-header tx-medium bd-0 tx-white bg-indigo">
                    RDC Picklist File Upload

                </div><!-- card-header -->
                <div class="widget-body">
                    <div class="widget-main">
                        <div id="fuelux-wizard-container" class="no-steps-container">
                            <form class="form-horizontal clearfix" action="@Url.Content("~/Transaction/RDCPicklistFileUpload")">


                                <div class="wizard-actions" style="text-align: right !important;margin-right: 70px;">



                                    <input type="hidden" id="hdautoid">
                                    <div class="col-lg-6" style="margin-left:10px;">

                                        <label style=" margin-right: 438px; margin-top: 35px;">
                                            File to upload:
                                        </label>
                                    </div>
                                    <div class="form-group col-lg-4 col-md-4 col-sm-4" style=" margin-left: 110px; margin-top: -38px;">
                                        <div class="col-lg-8">

                                            <input type="file" id="hf" name="postedFile" accept=".xlsx" autofocus style="background: #5fd6a6;color: white;margin-top: 7px;" />
                                        </div>
                                        <div class="control-label col-lg-4 no-padding-right" style="text-align: center !important;">

                                            <a class="btn btn-success btn-sm btn-next" data-original-title="Upload" title="Upload" id="btnupload" style="margin-left: 707px; margin-top: -39px; "> <span class="hidden-xs" id="actiontypeaddd">View</span></a>
                                        </div>
                                    </div>
                                    <a class="btn btn-success btn-custom waves-effect waves-light" id="btntemplateimport" style="margin-top: -118px;" type="submit" value="Export">
                                        Download Template
                                        <i class="fa fa-download" aria-hidden="true"></i>
                                    </a>
                                    <div class="form-group col-lg-4 col-md-4 col-sm-4">

                                    </div>
                                </div>

                            </form>
                            <div class="wizard-actions" style="text-align: center !important; margin-top: 50px; margin-bottom: 20px;">

                                <button class="btn btn-az-primary" id="btnsubmit" type="submit" tabindex="4">
                                    <span id="actiontype">Save</span>
                                    <i class="typcn typcn-bookmark icon-on-left"></i>
                                </button>
                                <button class="btn btn-danger btn-sm" type="submit" id="btnclear" tabindex="5">
                                    <span id="actiontype">Clear</span>
                                    <i class="typcn typcn-social-at-circular icon-on-left"></i>
                                </button>


                            </div>

                            <div class="scroll-x">
                                <table id="dynamic-tableChild" class="table table-striped table-bordered table-hover">
                                    <thead>
                                        <tr></tr>
                                    </thead>
                                    <tbody id="tbody"></tbody>
                                </table>
                            </div>
                            <div class="scroll-x">
                                <table id="dynamic-table" class="table table-striped table-bordered table-hover checkupload3">
                                    <thead>
                                        <tr></tr>
                                    </thead>
                                    <tbody id="tbody"></tbody>
                                </table>
                            </div>
                        </div>

                    </div><!-- /.widget-main -->
                </div><!-- /.widget-body -->
            </div>
        </div>
        <div class="hr hr-18 hr-double dotted col-lg-12"></div>

    </div>



</div>
