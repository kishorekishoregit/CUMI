@{
    ViewBag.Title = "WarehouseLocationMaster";
}
<div class="row">
    <div id="fuelux-wizard-container" class="no-steps-container">

        <div class="card bd-0">
            <div class="card-body bd bd-t-0">
                <form class="form-horizontal clearfix" id="WarehouseLocationMaster-form" name="myform" action="@Url.Content("~/Masters/WarehouseLocationMaster")" method="post">
                    <div class="card-header tx-medium bd-0 tx-white bg-indigo">
                        Warehouse Location Master

                    </div><!-- card-header -->
                    <div class="row row-sm" style=" margin-top: 25px;">
                     
                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">Plant<span class="text-danger">*</span>:</label>
                            @*<label class="control-label col-lg-5 no-padding-right" for="ddlplant">Plant:<span class="text-danger">*</span></label>*@
                            <div class="clearfix">
                                <input id="ddlplant" class="form-control" list="ddlplantlst" placeholder="Choose one" name="ddlplant" data-toggle="tooltip" data-placement="top" maxlength="50" tabindex="1" autofocus required>
                                <datalist id="ddlplantlst" style="background-color:aqua"></datalist>
                               
                            </div>
                        </div>
                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">Warehouse<span class="text-danger"></span>:</label>
                            <!--<label class="control-label col-lg-5 no-padding-right" for="ddlwarehouse">Warehouse:<span class="text-danger"></span></label>-->
                            @*<div class="col-lg-7">*@
                            <div class="clearfix">
                                <input id="ddlwarehouse" class="form-control" list="ddlwarehouse" name="ddlwarehouse" placeholder="Warehouse" data-toggle="tooltip" data-placement="top" maxlength="50" tabindex="2" readonly autofocus>
                                <datalist id="ddlwarehouse" style="background-color:aqua"></datalist>

                            </div>
                            @*</div>*@
                        </div>

                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">Location Code<span class="text-danger">*</span>:</label>
                            <input id="txtlocationcode" class="form-control" name="txtlocationcode" placeholder="Enter Location Code" type="text" required="" maxlength="50" autocomplete="off" tabindex="3" autofocus>
                            <input type="hidden" id="hdautoid">
                        </div>

                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">Location Name<span class="text-danger">*</span>:</label>
                            <input id="txtlocation" class="form-control" name="txtlocation" placeholder="Enter Location Name" type="text" required="" maxlength="50" autocomplete="off" tabindex="4" autofocus>
                            <input type="hidden" id="hdautoid">
                        </div>

                    </div>
                    <div class="row row-sm" style=" margin-top: 25px;">

                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <div id="slWrapper1" class="parsley-select">
                                <label class="control-label" for="ddlstatus">Record Status<span class="text-danger">*</span>:</label>
                                <select class="form-control select2" id="ddlstatus" data-parsley-class-handler="#slWrapper1" data-parsley-errors-container="#slErrorContainer1" autocomplete="off" tabindex="5" required="">
                                </select>
                            </div>
                            <div id="slErrorContainer1"></div>
                        </div>
                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">Remarks<span class="text-danger"></span>:</label>
                            <input id="txtRemarks" class="form-control" name="txtRemarks" placeholder="Enter Remarks" type="text" maxlength="50" autocomplete="off" tabindex="6" autofocus>
                            <input type="hidden" id="hdautoid">
                        </div>
                    </div>

                    <div class="card-body">
                        <div class="row">

                            <div class="col-lg-12">
                                <div class="card-box" style=" margin-top: 30px;">
                                    <div class="from-group col-lg-3 col-md-3 col-sm-3" style="float:right;margin-top: 20px;">
                                        <a class="btn btn-info btn-custom waves-effect waves-light" id="btntemplateimport"  value="Export">
                                            Download Template
                                            <i class="fa fa-download" aria-hidden="true"></i>
                                        </a>
                                    </div>


                                    <div class="form-group">
                                        <label for="FileUpload">Warehouse Location Master Details to Upload:</label>
                                    </div>

                                    <input type="file" id="hf" name="postedFile" title="upload file smaller than 5KB" placeholder="upload file smaller than 5KB" autofocus style="background: #5fd6a6;color: white;margin-top: 7px;" />
                                    <a class="btn btn-success btn-custom waves-effect waves-light" id="btnimport" value="Import" style="background: #7CB342;">
                                        Upload
                                        <i class="fas fa-file-upload" aria-hidden="true"></i>
                                    </a>




                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="wizard-actions" style="text-align: center !important; margin-top: 50px; margin-bottom: 20px;">
                        <button class="btn btn-az-secondary" id="btnsubmit" type="submit" title="Save" tabindex="8">
                            <span id="actiontype">Save</span>
                            <i class="typcn typcn-bookmark icon-on-left"></i>
                        </button>
                        <a class="btn btn-dark" id="btnprint" type="submit" title="Print" tabindex="7">
                            <span id="actiontype">Print</span>
                            <i class="typcn typcn-printer icon-on-left"></i>
                        </a>

                        <a class="btn btn-danger btn-sm" id="btnclear" title="Clear" tabindex="9">
                            <span id="actiontype">Clear</span>
                            <i class="typcn typcn-social-at-circular icon-on-left"></i>
                        </a>



                    </div>
                </form>
                <div class="card-header tx-medium bd-0 tx-white bg-indigo" style="border-radius: 5px;">
                    Warehouse Location Master Details
                </div><!-- card-header -->
                <div id="dynamic-table_wrapper" class="dataTables_wrapper no-footer" style="margin-top: 20px;">
                    <table id="dynamic-table" class="table table-striped table-bordered table-hover checkupload">
                    </table>
                </div>
            </div>
        </div><!-- card-body -->


    </div>
</div>

<div class="modal" id="printmodalview">
    <div class="modal-dialog modal-lg">
        <div class="modal-content" style="padding:12px;">
            <div class="popup_header">
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>

                <h3 class="header smaller lighter blue">Warehouse Location Details</h3>

            </div>

            <div class="modal-body">
                <div class="tabbable">
                    <div class="panel-body">
                        <div class="row">
                            <!-- begin col-6 -->
                            <div class="col-lg-3 col-md-4 col-sm-6">
                                <div class="form-group">
                                    <label class="control-label">Select All</label>
                                    <input type="checkbox" id="checkall" checked="checked" class="checkbox isselect" value="true" />
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-4 col-sm-6">
                                <div class="form-group">


                                </div>
                            </div>
                            <div class="col-lg-3 col-md-4 col-sm-6">

                            </div>

                            <div class="col-lg-3 col-md-4 col-sm-6">
                                <div class="form-group">
                                    <a href="javascript:;" class="btn btn-xs btn-danger" data-original-title="Print" title="Print" id="btnprintview"><i class="fa fa-print"></i> <span class="hidden-xs" style="float:right;">Print</span></a>

                                </div>
                            </div>


                            <!-- end row -->
                        </div>

                    </div>
                    <div class="scroll-x table-responsive">

                        <div id="upload" class="tab-pane active">
                            <div id="fuelux-wizard-container" class="no-steps-container">
                                <form class="form-horizontal clearfix" id="" name="myform" method="post">

                                    <div class="col-xs-12 clearfix">


                                        <div class="scroll-x">
                                            <table id="dynamic-printtableDetails" class="table table-striped table-bordered table-hover checkprint">
                                                <thead>
                                                    <tr></tr>
                                                </thead>
                                                <tbody id="tbody"></tbody>

                                            </table>
                                        </div>

                                    </div>
                                </form>
                            </div>
                        </div>



                    </div>
                </div>
            </div>
            <div class="modal-footer">

            </div>
        </div>
    </div>
</div>

<style>
    #printable * {
        color: #000;
        font-size: 7px;
        font-family: Arial, sans-serif;
    }



    div#printable {
        width: 2.65in;
        height: 2.00in;
    }

    #printable table {
        border-collapse: collapse;
    }

    #printable table, #printable td, #printable th {
        border: 1px solid black;
        padding: 4px;
    }
</style>

<style>
    #printableres * {
        color: #000;
        font-size: 7px;
        font-family: Arial, sans-serif;
    }



    div#printableres {
        width: 2.65in;
        height: 2.00in;
    }

    #printableres table {
        border-collapse: collapse;
    }

    #printableres table, #printableres td, #printableres th {
        border: 1px solid black;
        padding: 4px;
    }
</style>

<div style="visibility:hidden;overflow:auto;max-height:0px;max-width:0px;">
    <div id="printable" style="border:1px solid #000;">


    </div>
</div>

