@{
    ViewBag.Title = "WarehouseMaster";
}
<div class="row">
    <div id="fuelux-wizard-container" class="no-steps-container">

        <div class="card bd-0">
            <div class="card-body bd bd-t-0">
                <form class="form-horizontal clearfix" id="WarehouseMaster-form" name="myform" action="@Url.Content("~/Masters/WarehouseMaster")" method="post">
                    <div class="card-header tx-medium bd-0 tx-white bg-indigo">
                        Warehouse Master

                    </div><!-- card-header -->
                    <div class="row row-sm" style=" margin-top: 25px;">

                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">Warehouse Code<span class="text-danger">*</span>:</label>
                            <input id="txtwhcode" class="form-control" name="txtwhcode" placeholder="Enter Warehouse Code" type="text" required="" maxlength="50" autocomplete="off" tabindex="1" autofocus>
                            <input type="hidden" id="hdautoid">
                        </div>
                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">Warehouse Name<span class="text-danger">*</span>:</label>
                            <input id="txtwhname" class="form-control" name="txtwhname" placeholder="Enter Warehouse Name" type="text" required="" maxlength="50" autocomplete="off" tabindex="2" autofocus>
                            <input type="hidden" id="hdautoid">
                        </div>
                        @*<div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">*@

                        <div class="col-md-5 col-lg-3 ">
                            <label class="control-label" for="ddlitemcode">Plant<span class="text-danger">*</span>:</label>
                            <input type="text" id="ddlplant" list="ddlplantlst" name="ddlplant" placeholder="Choose one" class="form-control" tabindex="3" maxlength="50" required>
                            <datalist id="ddlplantlst" style="background-color:aqua"></datalist>

                        </div>
                        @*</div>*@
                        @*<div class="col-md-5 col-lg-4 ">
                <label class="control-label" for="ddlitemcode">Customer Id<span class="text-danger">*</span>:</label>
                <input type="text" id="ddlcustomerid" list="ddlplant" name="ddlcustomerid" placeholder="Enter Customer Id" class="form-control" tabindex="0" maxlength="50" required>
                <datalist id="Listddlcustomerid" style="background-color:aqua"></datalist>

            </div>*@
                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">Supervisor<span class="text-danger">*</span>:</label>
                            <input id="txtsupervisor" class="form-control" name="txtsupervisor" placeholder="Enter Supervisor" type="text" required="" maxlength="50" autocomplete="off" tabindex="4" autofocus>
                            <input type="hidden" id="hdautoid">
                        </div>

                    </div>

                    <div class="row row-sm" style=" margin-top: 25px;">

                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">Address1<span class="text-danger">*</span>:</label>
                            <input id="txtaddress1" class="form-control" name="txtaddress1" placeholder="Enter Address1" type="text" required="" maxlength="50" autocomplete="off" tabindex="5" autofocus>
                            <input type="hidden" id="hdautoid">
                        </div>
                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">Address2<span class="text-danger"></span>:</label>
                            <input id="txtaddress2" class="form-control" name="txtaddress2" placeholder="Enter Address2" type="text" maxlength="50" autocomplete="off" tabindex="6" autofocus>
                            <input type="hidden" id="hdautoid">
                        </div>
                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">Address3<span class="text-danger"></span>:</label>
                            <input id="txtaddress3" class="form-control" name="txtaddress3" placeholder="Enter Address3" type="text" maxlength="50" autocomplete="off" tabindex="7" autofocus>
                            <input type="hidden" id="hdautoid">
                        </div>
                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">City<span class="text-danger">*</span>:</label>
                            <input id="txtcity" class="form-control" name="txtcity" placeholder="Enter City" type="text" required="" maxlength="50" autocomplete="off" tabindex="8" autofocus>
                            <input type="hidden" id="hdautoid">
                        </div>
                    </div>

                    <div class="row row-sm" style=" margin-top: 25px;">

                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">State<span class="text-danger">*</span>:</label>
                            <input id="txtstate" class="form-control" name="txtstate" placeholder="Enter State" type="text" required="" maxlength="50" autocomplete="off" tabindex="9" autofocus>
                            <input type="hidden" id="hdautoid">
                        </div>
                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">Pincode<span class="text-danger">*</span>:</label>
                            <input id="txtpincode" class="form-control" name="txtpincode" placeholder="Enter Pincode" type="text" required="" maxlength="7" autocomplete="off" tabindex="10" autofocus>
                            <input type="hidden" id="hdautoid">
                        </div>
                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <label class="control-label">Remarks<span class="text-danger"></span>:</label>
                            <input id="txtremarks" class="form-control" name="txtremarks" placeholder="Enter Remarks" type="text" maxlength="50" autocomplete="off" tabindex="11" autofocus>
                            <input type="hidden" id="hdautoid">
                        </div>
                        <div class="col-md-5 col-lg-3 mg-t-20 mg-md-t-0">
                            <div id="slWrapper1" class="parsley-select">
                                <label class="control-label" for="ddlstatus">Record Status<span class="text-danger">*</span>:</label>
                                <select class="form-control select2" id="ddlstatus" data-parsley-class-handler="#slWrapper1" data-parsley-errors-container="#slErrorContainer1" autocomplete="off" tabindex="12" required>
                                </select>
                            </div>
                            <div id="slErrorContainer1"></div>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="row">

                            <div class="col-lg-12">
                                <div class="card-box" style=" margin-top: 30px;">
                                    <div class="from-group col-lg-3 col-md-3 col-sm-3" style="float:right;margin-top: 20px;">
                                        <a class="btn btn-info btn-custom waves-effect waves-light" id="btntemplateimport" type="submit" value="Export">
                                            Download Template
                                            <i class="fa fa-download" aria-hidden="true"></i>
                                        </a>
                                    </div>


                                    <div class="form-group">
                                        <label for="FileUpload">Warehouse Master Details to Upload:</label>
                                    </div>

                                    <input type="file" id="hf" name="postedFile" title="upload file smaller than 5KB" placeholder="upload file smaller than 5KB" autofocus style="background: #5fd6a6;color: white;margin-top: 7px;" />
                                    <a class="btn btn-success btn-custom waves-effect waves-light" id="btnimport" value="Import" style="background: #7CB342;">
                                        Upload
                                        <i class="fas fa-file-upload" aria-hidden="true"></i>
                                    </a>
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="wizard-actions" style="text-align: center !important; margin-top: 50px; margin-bottom: 20px;">

                        <button class="btn btn-az-secondary" id="btnsubmit" type="submit" title="Save" tabindex="13">
                            <span id="actiontype">Save</span>
                            <i class="typcn typcn-bookmark icon-on-left"></i>
                        </button>
                        <a class="btn btn-danger btn-sm" id="btnclear" title="Clear" tabindex="14">
                            <span id="actiontype">Clear</span>
                            <i class="typcn typcn-social-at-circular icon-on-left"></i>
                        </a>



                    </div>
                </form>
                <div class="card-header tx-medium bd-0 tx-white bg-indigo" style="border-radius: 5px;">
                    Warehouse Master Details
                </div><!-- card-header -->
                <div id="dynamic-table_wrapper" class="dataTables_wrapper no-footer" style="margin-top: 20px;">
                    <table id="dynamic-table" class="table table-striped table-bordered table-hover checkupload">
                    </table>
                </div>
            </div>
        </div><!-- card-body -->


    </div>
</div>





